{
  "name": "formik-components",
  "version": "0.3.0",
  "author": "Jannik Buschke",
  "description": "Utility components for formik",
  "displayName": "formik-components",
  "keywords": [
    "react",
    "formik",
    "form",
    "utils"
  ],
  "sideEffects": false,
  "license": "MIT",
  "private": false,
  "files": [
    "lib",
    "src",
    "es"
  ],
  "module": "./es/index.js",
  "main": "./lib/index.js",
  "types": "./lib/types.d.ts",
  "scripts": {
    "build": "tsc && tsc -p tsconfig.es.json",
