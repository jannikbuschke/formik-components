    "prettier": "prettier --write src/**/*.* package.json"
  },
  "dependencies": {},
  "peerDependencies": {
    "formik": ">= 1.4.1 < 2",
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17",
    "react-sortable-hoc": ">= 1.0.0 < 2"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.11",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "formik": "2.0.3",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-sortable-hoc": "1.10.1",
    "tslib": "1.10.0",
    "typescript": "3.6.3",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "prettier": "1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jannikbuschke/formik-components/"
  }
